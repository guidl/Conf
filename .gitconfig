[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  sf = svn fetch --all
  lg = log -p
  lol = log --pretty=format:'%C(yellow)%h %C(red)%ad %C(blue)%an%C(auto)%d %s' --date=human --graph
  lola = log --pretty=format:'%C(yellow)%h %C(red)%ad %C(blue)%an%C(auto)%d %s' --date=human --graph --all
  ls = ls-files
  tags = tag -l -n10
  # Show files ignored by git:
  ign = ls-files -o -i --exclude-standard
[core]
  autocrlf = input
  longpaths = true
  editor = vim
